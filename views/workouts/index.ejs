<%- include('../partials/header') %>

<table id="list">
    <thead>
        <tr>
            <th>Moves</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% workouts.forEach(function(w) {%>
            <tr>
                <td><%= w.name %></td>
            </tr>
            <tr>
                <td><%= w.day %></td>
            </tr>
            <tr>
                <td><%= w.description %></td>
            </tr>
            <tr>
                <td><%= w.muscleGroup %></td>
            </tr>
            <% }) %>
    </tbody>

</table>

</body>